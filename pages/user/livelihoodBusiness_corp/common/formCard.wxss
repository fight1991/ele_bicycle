/* pages/user/livelihoodBusiness_corp/common/formCard.wxss */
.box {
  background-color: #fff;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
  /* padding: 10rpx 0; */
}
.head, .middle, .foot {
  padding: 20rpx 20rpx 20rpx 30rpx;
}
.head, .middle {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.middle, .foot {
  color: #808080;
  font-size: 24rpx;
}
.foot {
  text-align: right;
  border-top: 1px solid #f5f5f5;
}
.head-right {
  color: #3C79FF;
}
.auditing {
  color: #3C79FF;
}
.waitInstall {
  color: #2EDB6F;
}
.failure {
  color: #FF7F21;
}
